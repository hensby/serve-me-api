{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "bookshelf",
      "settings": {
        "client": "mysql",
        "database": "${process.env.DB_NAME}",
        "host": "${process.env.DB_URL}",
        "username": "${process.env.DB_USER}",
        "password": "${process.env.DB_PASSWORD}"
      },
      "options": {
        "debug": false
      }
    }
  }
}
